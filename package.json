{
  "name": "amd-specs",
  "version": "0.0.0",
  "description": "An AMD equivalent for Intel's ARK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markasoftware/amd-specs.git"
  },
  "dependencies": {
    "babel-cli": "^6",
    "babel-plugin-array-includes": "^2",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.3",
    "csso-cli": "^1.1.0",
    "debug": "^3.2.6",
    "escape-string-regexp": "^1.0.5",
    "find": "^0.2.9",
    "is-glob": "^4.0.0",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "memoize-immutable": "^3.0.0",
    "micromatch": "^3.1.10",
    "mithril": "^1.1.6",
    "mixedtuplemap": "^1.0.0",
    "natural-compare": "^1.4.0",
    "papaparse": "^4.6.1",
    "sw-precache": "^5.2.0",
    "tap-summary": "^4.0.0",
    "tape": "^4.9.1",
    "uglify-js": "^3.4.9"
  },
  "author": "Mark Polyakov",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/markasoftware/SpecDB/issues"
  },
  "homepage": "https://github.com/markasoftware/SpecDB",
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "loose": true
        }
      ]
    ],
    "plugins": [
      "array-includes",
      [
        "transform-es2015-for-of",
        {
          "loose": true
        }
      ]
    ]
  }
}
